{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "import/no-anonymous-default-export": "off",
    "no-unused-vars": [
      "warn",
      { "vars": "all", "args": "all", "ignoreRestSiblings": true }
    ]
  }
}
